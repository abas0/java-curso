import java.util.Locale;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int x = sc.nextInt();
		int y = sc.nextInt();
		while(x != 0 && y != 0) {
			System.out.println(x + " " + y);
			if( x >= 0 || y>= 0 ) {
				System.out.println("primeiro quadrante");
			}
			/*else if() {
				
			}
			else if() {
				
			}
			else {
				
			}*/
			x = sc.nextInt();
			y = sc.nextInt();
		}
		sc.close();	
	} 

}
